{
  "name": "lagden-utils",
  "version": "0.4.0",
  "description": "Some utilities",
  "keywords": [
    "utilities",
    "utils",
    "lib",
    "helpers"
  ],
  "bug": {
    "url": "https://github.com/lagden/utils/issues",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "http://lagden.in"
  },
  "files": [
    "dist",
    "es6"
  ],
  "repository": "lagden/utils",
  "scripts": {
    "codecov-update": "cat ./coverage/coverage.json | codecov.io.js",
    "coverage": "istanbul cover _mocha -- --compilers js:babel/register --reporter spec",
    "pretest": "xo",
    "test": "npm run coverage",
    "start": "gulp watch"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "codecov.io": "^0.1.6",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-xo": "^0.4.0",
    "istanbul": "^0.4.0",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.3",
    "should": "^7.1.0",
    "xo": "^0.10.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "xo": {
    "envs": [
      "es6"
    ],
    "esnext": true,
    "ignores": [
      "dist/**"
    ],
    "global": [
      "window",
      "document",
      "HTMLElement"
    ],
    "rules": {
      "prefer-reflect": [
        2,
        {
          "exceptions": [
            "apply",
            "call"
          ]
        }
      ],
      "new-cap": [
        2,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ]
    }
  }
}
