{
  "name": "lagden-utils",
  "version": "1.0.0",
  "description": "Some utilities",
  "keywords": [
    "utilities",
    "utils",
    "lib",
    "helpers"
  ],
  "bug": {
    "url": "https://github.com/lagden/utils/issues",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "http://lagden.in"
  },
  "repository": "lagden/utils",
  "scripts": {
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=lcov | codecov",
    "pretest": "xo",
    "test": "nyc ava",
    "start": "gulp watch"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "xo": {
    "envs": [
      "es6"
    ],
    "esnext": true,
    "ignores": [
      "dist/**"
    ],
    "global": [
      "window",
      "document",
      "HTMLElement"
    ],
    "rules": {
      "prefer-reflect": [
        2,
        {
          "exceptions": [
            "apply",
            "call"
          ]
        }
      ],
      "new-cap": [
        2,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ]
    }
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel": "^6.5.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "codecov": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-xo": "^0.7.0",
    "jsdom": "^8.0.2",
    "nyc": "^5.6.0",
    "xo": "^0.12.1"
  },
  "ava": {
    "files": [
      "test/index.js"
    ],
    "failFast": true,
    "serial": true,
    "verbose": true
  },
  "babel": {
    "presets": [
      "stage-3",
      "es2015"
    ],
    "plugins": [
      "transform-es2015-modules-umd"
    ],
    "ignore": [
      "dist",
      "test"
    ]
  }
}
