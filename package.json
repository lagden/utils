{
  "name": "lagden-utils",
  "version": "3.0.2",
  "description": "Some utilities",
  "keywords": [
    "utilities",
    "utils",
    "lib",
    "helpers"
  ],
  "bug": {
    "url": "https://github.com/lagden/utils/issues",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "http://lagden.in"
  },
  "main": "index.js",
  "jsnext:main": "index.js",
  "repository": "lagden/utils",
  "scripts": {
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=lcov | codecov",
    "pretest": "xo",
    "test": "BABEL_ENV=ava nyc ava",
    "rollup": "BABEL_ENV=rollup rollup -c rollup.config.js",
    "pretty": "esformatter -i --indent.value=\"\t\" 'dist/*.js'",
    "build": "npm test && npm run rollup && npm run pretty"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "xo": {
    "ignore": [
      "dist/*.js"
    ],
    "envs": [
      "es6"
    ],
    "esnext": true,
    "global": [
      "window",
      "document",
      "HTMLElement",
      "SVGElement"
    ]
  },
  "babel": {
    "env": {
      "rollup": {
        "presets": [
          "es2015-rollup"
        ]
      },
      "ava": {
        "presets": [
          "es2015"
        ]
      }
    }
  },
  "ava": {
    "files": [
      "test/index.js"
    ],
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "failFast": true,
    "cacheEnabled": false
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel": "6.5.2",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-rollup": "1.1.1",
    "babel-register": "6.9.0",
    "codecov": "1.0.1",
    "esformatter": "0.9.5",
    "jsdom": "9.2.1",
    "nyc": "6.4.4",
    "rollup": "0.31.1",
    "rollup-plugin-babel": "2.5.1",
    "xo": "0.15.1"
  }
}
